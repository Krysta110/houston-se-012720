<%= form_for @movie do |f|  %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :year %>
<%= f.text_field :year %>

<%= f.label :actors %>
<%= f.collection_select :actor_ids, @actors, :id, :name%>

<%= f.fields_for :characters do |c| %>
<%= c.text_field :name %>
<% end %>

<%= f.submit %>

<% end %>